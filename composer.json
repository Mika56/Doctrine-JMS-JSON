{
    "name": "webit/doctrine-jms-json",
    "type": "library",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel@bojdo.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jms/serializer": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "*",
        "doctrine/dbal": "~2.2"
    },
    "suggest": {
        "doctrine/dbal": "To support JMS Json type with DBAL"
    },
    "autoload": {
        "psr-4": {
            "Webit\\DoctrineJmsJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\DoctrineJmsJson\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
